<Scene width="MATCH_PARENT" height="MATCH_PARENT" background-color="transparent">
    <LinearBox direction="HORIZONTAL" width="MATCH_PARENT" height="MATCH_PARENT" vertical-align="top">
        <LinearBox direction="VERTICAL" horizontal-align="LEFT" width="MATCH_PARENT"
                   padding-top="32dp" padding-left="32dp" padding-right="32dp" padding-bottom="32dp">
            <LinearBox direction="HORIZONTAL">
                <LinearBox direction="VERTICAL">
                    <Label text="Clear"/><ImageView id="img0" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Src"/><ImageView id="img1" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Dst"/><ImageView id="img2" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Src Over"/><ImageView id="img3" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Dst Over"/><ImageView id="img4" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Src In"/><ImageView id="img5" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Dst In"/><ImageView id="img6" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Src Out"/><ImageView id="img7" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Dst Out"/><ImageView id="img8" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Src Atop"/><ImageView id="img9" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Dst Atop"/><ImageView id="img10" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
                <LinearBox direction="VERTICAL" margin-left="8dp">
                    <Label text="Xor"/><ImageView id="img11" width="64dp" height="64dp" border-width="1dp" border-color="black"/>
                </LinearBox>
            </LinearBox>
        </LinearBox>
