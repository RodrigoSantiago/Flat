                <Button margin-left="16dp" text="Contained" style="button-icon-right button-contained-primary button-dense" icon="/default/icons/star.svg"/>
                <Button margin-left="16dp" text="Outlined" style="button-icon-right button-outlined-primary button-dense" icon="/default/icons/star.svg"/>
                <Button margin-left="16dp" text="Text" style="button-icon-right button-text-primary button-dense" icon="/default/icons/star.svg"/>
            </LinearBox>
            <Label text="Dense Icons" text-size="16sp" margin-left="4dp" margin-top="16dp"/>
            <LinearBox direction="HORIZONTAL" horizontal-align="LEFT">
                <Button style="icon-dense button-contained" icon="/default/icons/star.svg"/>
