                <Button text="Contained" style="button-contained button-dense"/>
                <Button margin-left="16dp" text="Contained" style="button-contained-primary button-dense"/>
                <Button margin-left="16dp" text="Outlined" style="button-outlined-primary button-dense"/>
                <Button margin-left="16dp" text="Text" style="button-text-primary button-dense"/>
            </LinearBox>

            <Label text="Dense Trailing Icons Left" text-size="16sp" margin-left="4dp" margin-top="16dp"/>
