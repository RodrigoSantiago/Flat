                <Button margin-left="16dp" text="Text" style="button-icon-right button-text-primary" icon="/default/icons/star.svg"/>
            </LinearBox>
            <Label text="Icons" text-size="16sp" margin-left="4dp" margin-top="16dp"/>
            <LinearBox direction="HORIZONTAL" horizontal-align="LEFT">
                <Button style="icon button-contained" icon="/default/icons/star.svg" />
                <Button margin-left="16dp" style="icon button-contained-primary" icon="/default/icons/star.svg" />
                <Button margin-left="16dp" style="icon button-outlined-primary" icon="/default/icons/star.svg"/>
                <Button margin-left="16dp" style="icon button-text-primary" icon="/default/icons/star.svg"/>
            </LinearBox>
            <Label text="Dense" text-size="16sp" margin-left="4dp" margin-top="16dp"/>
